{
  "manifest_version": 2,
  "name": "Kowal New Tab",
  "version": "1.0",
  "description": "My personal new tab",
  "author": "Krystian Kowal",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "topSites",
    "http://homeassistant.local:8123/",
    "https://api.openweathermap.org/",
    "chrome://favicon/size/64@1x"
  ],
  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"
  }
}